	            text: 'Monthly Average Humidity'
	        },
	        
	         xAxis: {
	        type: 'datetime'
	        },
	        yAxis: {
	            title: {
	                text: 'Humidity'
	            },
	            labels: {
	                formatter: function () {
	                    return this.value + ' rH';
	                }
	            }
	        },
	        tooltip: {
	            crosshairs: true,
	            shared: true
	        },
	        plotOptions: {
	            spline: {
	                marker: {
	                    radius: 4,
	                    lineColor: '#666666',
	                    lineWidth: 1
	                }
	            }
	        },
	        series: [{
	            name: 'Humidity',
	            marker: {
	                symbol: 'square'
	            },
	            data: minObj[0][chart1].dates

	        }]
	    });
	});
		   $(function () {
	    $('#flotchart').highcharts({
	        chart: {
	            type: 'spline'
	        },
	        title: {
	            text: 'Weekly Minimum and Maximum Humidity'
	        },
	       
	        xAxis: {
	            type: 'datetime',
	            
	            title: {
	                text: 'Date'
	            }
	        },
	        yAxis: {
	            title: {
	                text: 'Humidity'
	            },
	            labels: {
	                formatter: function () {
	                    return this.value + ' rH';
	                }
	            }
	        },
	        tooltip: {
	            headerFormat: '<b>{series.name}</b><br>',
	            pointFormat: '{point.x:%e. %b}: {point.y:.2f}'
	        },

	        plotOptions: {
	            spline: {
	                marker: {
	                    enabled: true
	                }
	            }
	        },

	        series: [{
	            name: 'Humidity Max in last 1 week',
	            
	            data: minObj[1][chart2].maxO
	        }, {
	            name: 'Humidity Min in last 1 week',
	            data:  minObj[1][chart2].minO
	        }
	        
	        ]
