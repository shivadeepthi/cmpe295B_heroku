            }); 
               socket.on('Pressure', function(data) {
                $('#pressure').html(parseInt(data.press));
                $.ajax( { url: "https://api.mongolab.com/api/1/databases/cmpe295b_siemt/collections/pressure?apiKey=rvu6pAb1B5s-xO1eQBgXKTWMzRQOHQ-0",
